#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chester/code/sst-monorepo/node_modules/.pnpm/sst-aws-cdk@2.91.0/node_modules/sst-aws-cdk/bin/node_modules:/Users/chester/code/sst-monorepo/node_modules/.pnpm/sst-aws-cdk@2.91.0/node_modules/sst-aws-cdk/node_modules:/Users/chester/code/sst-monorepo/node_modules/.pnpm/sst-aws-cdk@2.91.0/node_modules:/Users/chester/code/sst-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chester/code/sst-monorepo/node_modules/.pnpm/sst-aws-cdk@2.91.0/node_modules/sst-aws-cdk/bin/node_modules:/Users/chester/code/sst-monorepo/node_modules/.pnpm/sst-aws-cdk@2.91.0/node_modules/sst-aws-cdk/node_modules:/Users/chester/code/sst-monorepo/node_modules/.pnpm/sst-aws-cdk@2.91.0/node_modules:/Users/chester/code/sst-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sst-aws-cdk/bin/cdk" "$@"
else
  exec node  "$basedir/../sst-aws-cdk/bin/cdk" "$@"
fi
